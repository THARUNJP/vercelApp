{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build"
      }
    ],
    "rewrites": [
      {
        "source": "/((?!assets|favicon.ico|robots.txt|manifest.webmanifest|.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|json|woff2|ttf|map)).*)",
        "destination": "/index.html"
      }
    ]
  }
  